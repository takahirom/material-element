<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:width="300dp"
            android:height="300dp"
            android:viewportWidth="24"
            android:viewportHeight="24"
            android:alpha="1">
            <group
                android:name="menu"
                android:pivotX="12"
                android:pivotY="12">
                <group
                    android:name="bottom_container"
                    android:pivotX="20"
                    android:pivotY="17">
                    <path
                        android:name="bottom"
                        android:pathData="M4,17 L20,17"
                        android:strokeColor="#000"
                        android:strokeWidth="2"
                        android:strokeLineCap="square"/>
                </group>
                <group
                    android:name="stem_container"
                    android:pivotX="12"
                    android:pivotY="12">
                    <path
                        android:name="stem"
                        android:pathData="M4,12 L20,12"
                        android:strokeColor="#000"
                        android:strokeWidth="2"
                        android:strokeLineCap="square"/>
                </group>
                <group
                    android:name="top_container"
                    android:pivotX="20"
                    android:pivotY="7">
                    <path
                        android:name="top"
                        android:pathData="M4,7 L20,7"
                        android:strokeColor="#000"
                        android:strokeWidth="2"
                        android:strokeLineCap="square"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="top_container">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android">
                <objectAnimator
                    android:name="top_container"
                    android:propertyName="translateX"
                    android:startOffset="500"
                    android:duration="700"
                    android:valueFrom="0"
                    android:valueTo="-1.41421356"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:name="top_container"
                    android:propertyName="translateY"
                    android:startOffset="500"
                    android:duration="700"
                    android:valueFrom="0"
                    android:valueTo="5"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:name="top_container"
                    android:propertyName="rotation"
                    android:startOffset="500"
                    android:duration="700"
                    android:valueFrom="0"
                    android:valueTo="45"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="bottom_container">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android">
                <objectAnimator
                    android:name="bottom_container"
                    android:propertyName="translateX"
                    android:startOffset="500"
                    android:duration="700"
                    android:valueFrom="0"
                    android:valueTo="-1.41421356"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:name="bottom_container"
                    android:propertyName="translateY"
                    android:startOffset="500"
                    android:duration="700"
                    android:valueFrom="0"
                    android:valueTo="-5"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:name="bottom_container"
                    android:propertyName="rotation"
                    android:startOffset="500"
                    android:duration="700"
                    android:valueFrom="0"
                    android:valueTo="-45"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="menu">
        <aapt:attr name="android:animation">
            <objectAnimator
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:name="menu"
                android:propertyName="rotation"
                android:startOffset="500"
                android:duration="700"
                android:valueFrom="0"
                android:valueTo="180"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="bottom">
        <aapt:attr name="android:animation">
            <objectAnimator
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:name="bottom"
                android:propertyName="trimPathStart"
                android:startOffset="500"
                android:duration="700"
                android:valueFrom="0"
                android:valueTo="0.5"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="top">
        <aapt:attr name="android:animation">
            <objectAnimator
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:name="top"
                android:propertyName="trimPathStart"
                android:startOffset="500"
                android:duration="700"
                android:valueFrom="0"
                android:valueTo="0.5"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="stem">
        <aapt:attr name="android:animation">
            <objectAnimator
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:name="stem"
                android:propertyName="trimPathEnd"
                android:startOffset="500"
                android:duration="700"
                android:valueFrom="1"
                android:valueTo="0.8"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="stem_container">
        <aapt:attr name="android:animation">
            <objectAnimator
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:name="stem_container"
                android:propertyName="translateX"
                android:startOffset="500"
                android:duration="700"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
        </aapt:attr>
    </target>
</animated-vector>
